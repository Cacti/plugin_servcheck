<servcheck>
	<name>Servcheck Data Query</name>
	<script_path>|path_cacti|/scripts/ss_servcheck.php</script_path>
	<script_function>ss_servcheck</script_function>
	<script_server>php</script_server>
	<arg_prepend></arg_prepend>
	<arg_index>index</arg_index>
	<arg_query>query</arg_query>
	<arg_get>get</arg_get>
	<output_delimiter>!</output_delimiter>
	<index_order>servcheck_id</index_order>
	<index_order_type>numeric</index_order_type>
	<index_title_format>Check#|chosen_order_field|</index_title_format>

	<fields>
		<servcheck_id>
			<name>Test Id</name>
			<direction>input</direction>
			<query_name>servcheck_id</query_name>
		</servcheck_id>
		<servcheck_name>
			<name>Test Name</name>
			<direction>input</direction>
			<query_name>servcheck_name</query_name>
		</servcheck_name>
		<attempt>
			<name>Attemp</name>
			<direction>output</direction>
			<query_name>attempt</query_name>
		</attempt>
		<duration>
			<name>Duration</name>
			<direction>output</direction>
			<query_name>duration</query_name>
		</duration>
		<result>
			<name>Result</name>
			<direction>output</direction>
			<query_name>result</query_name>
		</result>
	</fields>
</servcheck>